TeleportGroupBox:AddButton({
    Text = "Teleport",
    Func = function()
        if not selectedTeleportName or not SelectedLocation then
            Library:Notify("No location selected!", 3)
            return
        end

        local character = player.Character
        local humanoidRootPart = character and character:FindFirstChild("HumanoidRootPart")
        if not humanoidRootPart then
            Library:Notify("Teleport failed: missing HumanoidRootPart", 3)
            return
        end

        local gui = createTeleportGui()

        -- Try to safely enable SwimMethod (wait for it to take effect)
        getgenv().SwimMethod = true
        task.wait(1)

        -- Extra safety check to ensure HRP is still valid after wait
        humanoidRootPart = character and character:FindFirstChild("HumanoidRootPart")
        if not humanoidRootPart then
            getgenv().SwimMethod = false
            removeTeleportGui(gui)
            Library:Notify("Teleport failed: HumanoidRootPart lost", 3)
            return
        end

        local success, err = pcall(function()
            -- Force a physics-ready pause before setting CFrame
            humanoidRootPart.Velocity = Vector3.zero
            humanoidRootPart.CFrame = CFrame.new(SelectedLocation)
        end)

        -- Wait a bit after teleport to help physics settle
        task.wait(2)

        -- After teleport, disable swim method ONLY if enhancedWalk is NOT enabled
        if not enhancedWalk then
            getgenv().SwimMethod = false
        end

        removeTeleportGui(gui)

        if success then
            Library:Notify("Teleported to " .. selectedTeleportName .. " safely!", 3)
        else
            Library:Notify("Teleport failed: " .. tostring(err), 3)
        end
    end,
    Tooltip = "Teleports to the selected location",
})





