local function tableKeys(tbl)
    local keys = {}
    for key in pairs(tbl) do
        table.insert(keys, key)
    end
    return keys
end


getgenv().SwimMethod = false


local function createTeleportGui()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "TeleportingOverlay"
    screenGui.IgnoreGuiInset = true
    screenGui.ResetOnSpawn = false
    screenGui.DisplayOrder = 999

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.BackgroundColor3 = Color3.new(0, 0, 0)
    frame.BackgroundTransparency = 0
    frame.Parent = screenGui

    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = "VOID.CC"
    textLabel.TextColor3 = Color3.new(300, 0,0)
    textLabel.TextScaled = true
    textLabel.Font = Enum.Font.SourceSansBold
    textLabel.Parent = frame

    screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    return screenGui
end

local function removeTeleportGui(gui)
    if gui and gui:FindFirstChild("Frame") then
        local frame = gui.Frame
        local textLabel = frame:FindFirstChildOfClass("TextLabel")
        for i = 1, 10 do
            local t = i / 10
            frame.BackgroundTransparency = t
            if textLabel then
                textLabel.TextTransparency = t
            end
            task.wait(0.02)
        end
    end
    gui:Destroy()
end


